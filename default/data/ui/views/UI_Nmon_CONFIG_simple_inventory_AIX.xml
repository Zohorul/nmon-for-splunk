<form stylesheet="Home.css,singlevalue.css,ui_simple.css">
  <label>NMON CONFIG, AIX Simple Inventory</label>
  <description/>
  <fieldset autoRun="true" submitButton="false">
    <input type="time" searchWhenChanged="true" token="timerange">
      <label>Time Range:</label>
      <default>
        <earliestTime>-24h@h</earliestTime>
        <latestTime>now</latestTime>
      </default>
    </input>
    <input type="radio" token="datasource" searchWhenChanged="true">
      <label>Data Source:</label>
      <choice value="inputlookup nmon_inventory | search OStype=AIX">Based on Nmon Inventory Lookup</choice>
      <choice value="search `nmon_config` | search OStype=&quot;AIX&quot; | stats latest(AIX_Machine_SerialNumber) As Machine_SerialNumber, latest(AIX_virtualcpus) As virtualcpus, latest(AIX_logicalcores) As logicalcores,  latest(AIX_entitled) As entitled, latest(OStype) As OStype, latest(Processor) As Processor, latest(AIX_processor_mode) As AIX_processor_mode, latest(AIX_processor_clockspeed) As AIX_processor_clockspeed, latest(AIX_cpu_type) As AIX_cpu_type, latest(AIX_kernel_type) As AIX_kernel_type, latest(AIX_plateform_firmware_level) As AIX_plateform_firmware_level, latest(nmon_version) As nmon_version, latest(AIX_PoolID) As AIX_PoolID, latest(AIX_system_installed_CPUs) As AIX_system_installed_CPUs, latest(AIX_system_active_CPUs) As AIX_system_active_CPUs, latest(AIX_PoolCPUs) As AIX_PoolCPUs by hostname | fillnull value=Undeterminated">Based on Nmon Raw Data</choice>
      <default>inputlookup nmon_inventory | search OStype=AIX</default>
    </input>
  </fieldset>
  <searchTemplate>| $datasource$</searchTemplate>
  <earliestTime>$timerange.earliest$</earliestTime>
  <latestTime>$timerange.latest$</latestTime>
  <row>
    <html>
      <div style="text-align: center;">
        <img src="../../static/app/nmon/icons/grey_theme/inventory.png" alt="Inventory"/>
      </div>
      <div class="custom">
        <h1>AIX Inventory Summary</h1>
      </div>
    </html>
  </row>
  <row>
    <single>
      <searchPostProcess>stats dc(hostname) As count</searchPostProcess>
      <option name="afterLabel">Hosts known within this period</option>
      <option name="beforeLabel">Found</option>
      <option name="underLabel">AIX</option>
      <option name="linkView">search</option>
      <option name="drilldown">none</option>
    </single>
  </row>
  <row grouping="1,1">
    <chart>
      <title>AIX Version</title>
      <searchPostProcess>stats count by AIX_LEVEL</searchPostProcess>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">minimal</option>
      <option name="charting.drilldown">all</option>
      <option name="height">300px</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <table>
      <title>Table Stats</title>
      <searchPostProcess>| top limit=0 AIX_LEVEL | eval percent=round(percent,2) | rename count As "Number of Hosts" | strcat percent "%" percent | rename percent As "Percent (%)"</searchPostProcess>
    </table>
  </row>
  <row grouping="1,1">
    <chart>
      <title>Processor Type</title>
      <searchPostProcess>stats count by Processor</searchPostProcess>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
