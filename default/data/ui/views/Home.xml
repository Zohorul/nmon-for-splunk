      <single>
        <search base="Global">
	       <query>fields index</query>
	     </search>
        <option name="linkView">search</option>
        <option name="underLabel">DATA INDEX NAME</option>
        <option name="drilldown">none</option>
        <option name="drilldown">all</option>
        <option name="linkFields">result</option>
        <option name="linkSearch">| `indexes_datestats` | where (sourcetype == "nmon_data")</option>
      </single>
      <single>
        <search base="Global">
	       <query>fields First*</query>
	     </search>
        <option name="linkView">search</option>
        <option name="underLabel">DATE OF FIRST EVENT</option>
        <option name="drilldown">none</option>
        <option name="refresh.auto.interval">30</option>
        <option name="drilldown">all</option>
        <option name="linkFields">result</option>
        <option name="linkSearch">| `indexes_datestats` | where (sourcetype == "nmon_data")</option>
      </single>
      <single>
        <search>
	       <query>| `indexes_datestats` | where (sourcetype == "nmon_data") | fields Last*</query>
	     </search>
        <option name="linkView">search</option>
        <option name="underLabel">DATE OF LAST EVENT</option>
        <option name="drilldown">none</option>
        <option name="refresh.auto.interval">30</option>
        <option name="drilldown">all</option>
        <option name="linkFields">result</option>
        <option name="linkSearch">| `indexes_datestats` | where (sourcetype == "nmon_data")</option>
      </single>
      <single>
        <search ref="Hosts with data within last 7 days"></search>
        <option name="linkView">search</option>
        <option name="beforeLabel">There have been</option>
        <option name="afterLabel">Host(s) with activity</option>
        <option name="underLabel">IN THE LAST 7 DAYS</option>
        <option name="refresh.auto.interval">300</option>
        <option name="field">count</option>
        <option name="drilldown">all</option>
        <option name="linkFields">result</option>
        <option name="linkSearch">| pivot NMON_Config Nmon_Config last(AIX_Machine_SerialNumber) AS "AIX_Machine_SerialNumber" dc(hostname) AS "dcount" SPLITROW hostname AS hostname SORT 0 hostname ROWSUMMARY 0 COLSUMMARY 0 NUMCOLS 0 SHOWOTHER 0
| eval serialnum=if(isnull(AIX_Machine_SerialNumber), hostname, AIX_Machine_SerialNumber) | fields hostname,serialnum | stats count</option>
      </single>
      <single>
        <search ref="Volume of Data indexed Today"></search>
        <option name="linkView">search</option>
        <option name="beforeLabel">There have been</option>
        <option name="afterLabel">MB Indexed</option>
        <option name="underLabel">TODAY / EVOLUTION VS YESTERDAY</option>
        <option name="refresh.auto.interval">130</option>
        <option name="field">value</option>
        <option name="drilldown">all</option>
        <option name="linkFields">result</option>
        <option name="linkSearch">| savedsearch "Volume of Data indexed Today"</option>
      </single>
      <single>
        <search ref="Number of Errors in Data Processing since last 24 Hours"></search>
        <option name="linkView">search</option>
        <option name="afterLabel">Errors Reported</option>
        <option name="underLabel">IN THE LAST 24 HOURS</option>
        <option name="refresh.auto.interval">130</option>
        <option name="field">value</option>
        <option name="drilldown">all</option>
        <option name="linkFields">result</option>
        <option name="linkSearch">| savedsearch "Number of Errors in Data Processing since last 24 Hours"</option>
